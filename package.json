    "analyze-build": "cross-env ANALYZE=true next build",
    "build": "run-p build-*",
    "build-api": "tsc -P api",
    "build-client": "next build",
    "clean": "rm -rf logs/*",
    "dev": "cross-env NODE_ENV=development run-p -l watch-*",
    "start": "run-p start-client start-api",
    "start-api": "node api/dist/api/index.js",
    "start-client": "next start",
    "start-pm2": "pm2 start ecosystem.yaml",
    "start-pm2-dev": "pm2 start ecosystem-dev.yaml",
    "stop": "pm2 delete ecosystem-dev.yaml",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "watch-api": "tsc-watch -P api --onSuccess \"node --inspect=27050 api/dist/api/index.js\"",
    "watch-client": "wait-on tcp:4000 && next"
  },
